{
  "name": "agentoja-contact-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "AgentOja Contact Form Backend - Azure Functions",
  "main": "dist/functions/contact-form/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "prestart": "npm run build",
    "start": "func start",
    "start:dev": "npm run build && func start --cors *",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "setup": "npm install && npm run build",
    "dev": "concurrently \"npm run watch\" \"npm run start:dev\""
  },
  "keywords": [
    "azure-functions",
    "agentoja",
    "contact-form"
  ],
  "author": "AgentOja Team",
  "license": "MIT",
  "dependencies": {
    "@azure/communication-email": "^1.0.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/functions": "^4.5.0",
    "escape-html": "^1.0.3"
  },
  "devDependencies": {
    "@types/escape-html": "^1.0.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.10.0",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "azure-functions-core-tools": "^4.0.5455",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "jest": "^29.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.3"
  }
}
